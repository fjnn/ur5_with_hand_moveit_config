<?xml version="1.0" encoding="UTF-8"?>

<launch>

  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find ur5_with_hand_moveit_config)/human_description/urdf/human_arm.xacro'"/>

  <!-- Combine joint values -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher"/>

  <!-- Show in Rviz   -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find ur5_with_hand_moveit_config)/human_description/urdf/config_human.rviz"/>

  <!-- send joint values-->
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
    <param name="use_gui" value="False"/>
  </node>

</launch>
